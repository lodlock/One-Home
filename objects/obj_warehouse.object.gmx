            <string>/// Draw the resources menu

if(place_meeting(x,y,obj_player) || draw_resources){
    draw_set_halign(fa_left);
    draw_set_colour(c_navy);
    draw_roundrect(30,30,295,230,false);
    
    draw_set_colour(c_white);
    draw_roundrect(30,30,295,230,true);
    
    
    draw_set_font(fnt_Helvetica_bold);
    draw_text(50,50,"Resources:");
    
    i = 0 ;
    repeat(5){
        var type = global.ls_resources[i,0];
        
        switch(type){
            case "Wood":
                draw_sprite(spr_Wood_Mini,0,50,100+(i*25));
                break;
            case "Utilities":
                draw_sprite(spr_Utilities_Mini,0,50,100+(i*25));
                break;
            case "Metal":
                draw_sprite(spr_Metal_Mini,0,50,100+(i*25));
                break;
            case "Plastic":
                draw_sprite(spr_Plastic_Mini,0,50,100+(i*25));
                break;
            case "Glass / Stone":
                draw_sprite(spr_Glass_Stone_Mini,0,50,100+(i*25));
                break;
        }
    
        draw_set_font(fnt_Helvetica_bold);
        draw_text(60,90+(i*25),global.ls_resources[i,0])
        
        draw_set_font(fnt_Helvetica_Neue);
        draw_text(250,90+(i*25),global.ls_resources[i,1])
        
        i++;
    }
}
</string>
